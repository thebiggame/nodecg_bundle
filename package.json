{
  "name": "thebiggame",
  "version": "ng-48.0.0",
  "description": "theBIGGAME graphical assets, etc",
  "homepage": "http://thebiggame.org",
  "author": {
    "name": "duck",
    "email": "me@duck.me.uk",
    "url": "http://duck.me.uk"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "~2.1.0",
    "dashboardPanels": [
      {
        "name": "alert",
        "title": "Alert",
        "width": 3,
        "file": "alert.html",
        "headerColor": "#ff0000",
        "workspace": "Stage"
      },
      {
        "name": "countdown",
        "title": "Countdown",
        "width": 2,
        "file": "countdown.html",
        "headerColor": "#ff0000",
        "workspace": "Stage"
      },
      {
        "name": "halfwipe",
        "title": "Halfwipe",
        "width": 3,
        "file": "halfwipe.html",
        "headerColor": "#ff0000",
        "workspace": "Stream"
      },
      {
        "name": "lowerthird",
        "title": "Lower Third",
        "width": 3,
        "file": "lowerthird.html",
        "headerColor": "#ff0000",
        "workspace": "Stream"
      },
      {
        "name": "persons",
        "title": "Persons",
        "width": 2,
        "file": "persons.html",
        "headerColor": "#ff0000",
        "workspace": "Stream"
      },
      {
        "name": "scoreboard",
        "title": "Scoreboard",
        "width": 2,
        "file": "scoreboard.html",
        "headerColor": "#ff0000",
        "workspace": "Stream"
      },
      {
        "name": "ng-info",
        "title": "Information",
        "width": 4,
        "file": "ng-info.html",
        "headerColor": "#ff0000",
        "workspace": "NG Stage"
      },
      {
        "name": "projector-controls",
        "title": "Visibility Control",
        "width": 4,
        "file": "ng-proj-animate.html",
        "headerColor": "#000000",
        "workspace": "NG Stage"
      },
      {
        "name": "ng-alert",
        "title": "Alert",
        "width": 4,
        "file": "alert.html",
        "headerColor": "#ff0000",
        "workspace": "NG Stage"
      },
      {
        "name": "ng-countdown",
        "title": "Countdown",
        "width": 2,
        "file": "countdown.html",
        "headerColor": "#ff0000",
        "workspace": "NG Stage"
      }
    ],
    "graphics": [
      {
        "file": "stage.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "stream.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "ng-stage.html",
        "width": 1920,
        "height": 1080
      }
    ],
    "soundCues": [
      {
        "name": "alertCue",
        "defaultFile": "sound/alert.ogg",
        "channels": 1
      },
      {
        "name": "announcementCue",
        "defaultFile": "sound/announcement.ogg",
        "channels": 1
      }
    ],
    "assetCategories": [
      {
        "name": "stage-slides",
        "title": "Slideshow - Stage",
        "allowedTypes": [
          "jpg",
          "jpeg",
          "gif",
          "png"
        ]
      }
    ]
  },
  "repository": "https://github.com/thebiggame/tbgcg.git",
  "license": "Apache-2.0",
  "dependencies": {
    "net-snmp": "^3.19.*",
    "node-ical": "^0.20.1",
    "nodecg": "^2.5.3"
  }
}
