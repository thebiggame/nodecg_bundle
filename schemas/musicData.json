{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "name": {
      "type": "string",
      "default": "Unknown"
    },
    "album_name": {
      "type": "string",
      "default": "Unknown Album"
    },
    "album_art": {
      "type": "string"
    },
    "artist": {
      "type": "string",
      "default": ""
    },
    "party": {
      "type": "object",
      "properties": {
        "rating": {
          "type": "number",
          "default": 0
        },
        "userName": {
          "type": "string"
        }
      }
    }
  },
  "required": [
    "name",
    "artist"
  ]
}